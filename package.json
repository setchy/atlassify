{
  "name": "atlassify",
  "version": "0.0.0-forge-alpha",
  "private": true,
  "description": "Atlassian notifications on your menu bar.",
  "main": ".vite/build/index.js",
  "scripts": {
    "clean": "rimraf build coverage dist out node_modules .vite",
    "prepare": "husky",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "pnpm codegen --watch",
    "dev": "concurrently --names \"forge,codegen\" --prefix-colors \"blue,magenta\" \"electron-forge start\" \"pnpm codegen:watch\"",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make:linux": "electron-forge make --platform=linux",
    "make:macos": "electron-forge make --platform=darwin",
    "make:win": "electron-forge make --platform=win32",
    "electron-publish": "electron-forge publish",
    "lint": "biome check --fix",
    "lint:check": "biome check",
    "test": "vitest --coverage --run",
    "test:watch": "vitest --watch --coverage",
    "test:ui": "vitest --ui"
  },
  "engines": {
    "node": ">=24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/setchy/atlassify.git"
  },
  "keywords": [
    "atlassify",
    "atlassian",
    "bitbucket",
    "compass",
    "confluence",
    "jira",
    "notifications",
    "electron",
    "menubar",
    "taskbar",
    "tray"
  ],
  "author": {
    "name": "Adam Setch",
    "url": "https://github.com/setchy"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/setchy/atlassify/issues"
  },
  "homepage": "https://atlassify.io",
  "dependencies": {
    "@atlaskit/avatar": "25.7.4",
    "@atlaskit/avatar-group": "12.4.10",
    "@atlaskit/badge": "18.4.0",
    "@atlaskit/button": "23.9.6",
    "@atlaskit/checkbox": "17.3.3",
    "@atlaskit/css-reset": "7.3.12",
    "@atlaskit/form": "15.3.4",
    "@atlaskit/heading": "5.3.0",
    "@atlaskit/icon": "30.0.2",
    "@atlaskit/inline-message": "15.5.4",
    "@atlaskit/logo": "19.10.2",
    "@atlaskit/modal-dialog": "14.10.3",
    "@atlaskit/primitives": "18.0.0",
    "@atlaskit/radio": "8.4.2",
    "@atlaskit/select": "21.7.4",
    "@atlaskit/spinner": "19.0.10",
    "@atlaskit/textfield": "8.2.3",
    "@atlaskit/toggle": "15.2.9",
    "@atlaskit/tokens": "11.0.0",
    "@atlaskit/tooltip": "20.14.3",
    "@aptabase/electron": "0.3.1",
    "@heroicons/react": "2.2.0",
    "@tanstack/react-query": "5.90.21",
    "axios": "1.13.5",
    "clsx": "2.1.1",
    "dotenv": "17.3.1",
    "electron-log": "5.4.3",
    "electron-updater": "6.8.3",
    "fs-xattr": "^0.4.0",
    "i18next": "25.8.7",
    "i18next-browser-languagedetector": "8.2.1",
    "menubar": "9.5.2",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-i18next": "16.5.4",
    "react-router-dom": "7.13.0",
    "tailwind-merge": "3.4.0",
    "zustand": "5.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@compiled/vite-plugin": "1.1.2",
    "@discordapp/twemoji": "16.0.1",
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-deb": "^7.6.0",
    "@electron-forge/maker-dmg": "^7.6.0",
    "@electron-forge/maker-rpm": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.6.0",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-vite": "^7.6.0",
    "@electron-forge/publisher-github": "^7.6.0",
    "@graphql-codegen/cli": "6.1.1",
    "@graphql-codegen/schema-ast": "5.0.0",
    "@parcel/watcher": "2.5.6",
    "@swc-contrib/plugin-graphql-codegen-client-preset": "0.17.0",
    "@tailwindcss/postcss": "4.1.18",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "24.10.13",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/react-router-dom": "5.3.3",
    "@vitejs/plugin-react-swc": "4.2.3",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "concurrently": "9.2.1",
    "date-fns": "4.1.0",
    "electron": "40.4.1",
    "graphql": "16.12.0",
    "happy-dom": "20.6.1",
    "husky": "9.1.7",
    "nock": "14.0.11",
    "postcss": "8.5.6",
    "rimraf": "6.1.2",
    "tailwindcss": "4.1.18",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vite": "^6.0.11",
    "vite-plugin-checker": "0.12.0",
    "vite-plugin-static-copy": "3.2.0",
    "vitest": "4.0.18"
  },
  "packageManager": "pnpm@10.29.3",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@parcel/watcher",
      "@swc/core",
      "@tailwindcss/oxide",
      "electron",
      "electron-winstaller",
      "esbuild",
      "fs-xattr",
      "macos-alias",
      "unrs-resolver"
    ]
  },
  "lint-staged": {
    "*": "biome check --fix --no-errors-on-unmatched",
    "*.{js,ts,tsx}": "pnpm test --changed --passWithNoTests --update"
  }
}
